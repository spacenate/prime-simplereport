mutation addTestResult($deviceId: String!, $specimenId: String!, $result: String!, $patientId: ID!, $dateTested: DateTime) {
    addTestResultNew(
      patientId: $patientId,
      deviceId: $deviceId,
      specimenId: $specimenId,
      result: $result,
      dateTested: $dateTested
    ) {
      testResult {
        internalId,
        result,
        deviceType {
          internalId
        }
      }
      deliverySuccess
    }
  }